{
    "env": {
        "browser": true,
        "es2021": true
    },
    "globals": {
      "process": true
    },
    "extends": [
        "eslint:recommended",
        "plugin:vue/vue3-essential"
    ],
    "parserOptions": {
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "plugins": [
        "vue",
        "import"
    ],
    "rules": {
      "import/named": 2,
      "import/namespace": 0,
      "import/default": 0,
      "import/export": 2
    }
}
